{
  "name": "djjerry85/atol-online",
  "version": "4.1.1",
  "description": "Библиотека для работы с АТОЛ Онлайн. Fork it-quasar/atol-online",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "fix": "./vendor/bin/php-cs-fixer fix ./src && ./vendor/bin/php-cs-fixer fix ./tests",
    "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php --testdox tests"
  },
  "authors": [
    {
      "name": "Pavel Puchkov",
      "email": "0x6368656174@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ItQuasar\\AtolOnline\\": "src"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.3",
    "ext-curl": "*",
    "ext-json": "*",
    "psr/log": "^1.0|^2.0|^3.0",
    "psr/cache": "^1|^2|^3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.12",
    "phpunit/phpunit": "^7.5.20 || ^8.5.23 || ^9.5.13"
  }
}
